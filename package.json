{
  "name": "@termsurf/text",
  "version": "0.3.62",
  "scripts": {
    "toss": "rm -rf host",
    "make": "pnpm toss && tsc && tsc-alias && cp package.json host/code/package.json && cp code/tibetan/parser.js host/code/tibetan/parser.js",
    "scan": "concurrently --kill-others \"tsc -w\" \"tsc-alias -w\"",
    "test": "tsx test/make.test.ts",
    "host": "pnpm make && cd host/code && npm publish --access=public",
    "move:talk": "tsx code/convert-source-ipa-to-talk.ts"
  },
  "devDependencies": {
    "@termsurf/talk": "^3.1.6",
    "@termsurf/tone": "^3.3.4",
    "@trivago/prettier-plugin-sort-imports": "^4.0.0",
    "@types/lodash": "^4.17.7",
    "@types/node": "^18.11.17",
    "@types/prettier": "^2.7.2",
    "@typescript-eslint/eslint-plugin": "^5.46.1",
    "@typescript-eslint/parser": "^5.46.1",
    "concurrently": "^7.6.0",
    "csv-parse": "^5.5.6",
    "eslint": "^8.31.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-standard-with-typescript": "^24.0.0",
    "eslint-import-resolver-typescript": "^3.5.2",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-n": "^15.6.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-simple-import-sort": "^8.0.0",
    "eslint-plugin-sort-exports": "^0.8.0",
    "eslint-plugin-sort-keys": "^2.3.5",
    "eslint-plugin-typescript-sort-keys": "^2.1.0",
    "lodash": "^4.17.21",
    "prettier": "^2.8.1",
    "sort-keys-recursive": "^2.1.10",
    "ts-node": "^10.9.1",
    "tsc-alias": "^1.8.2",
    "tsx": "^3.12.7",
    "typescript": "^4.9.4"
  },
  "dependencies": {
    "tibetan-ewts-converter": "^1.0.1"
  }
}
